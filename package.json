{
	"name": "svelte-knobby",
	"version": "0.3.4",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm run package",
		"preview": "vite preview",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"package": "svelte-package && publint",
		"release": "pnpm run package && cd package && pnpm publish",
		"postversion": "git push && git push --tags && pnpm run release",
		"prepublishOnly": "echo \"Did you mean to run 'pnpm run release'?\" && exit 1"
	},
	"dependencies": {
		"bezier-easing": "^2.1.0",
		"svelte-undo": "^1.0.2",
		"yootils": "^0.3.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.20.2",
		"@sveltejs/package": "2.0.2",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "~2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "0.1.12",
		"svelte": "^3.59.1",
		"svelte2tsx": "^0.6.15",
		"typescript": "^5.1.3",
		"vite": "4.3.9"
	},
	"peerDependencies": {
		"svelte": "^3.34.0"
	},
	"license": "MIT",
	"type": "module",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	]
}
